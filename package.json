{
  "name": "reactRecipesProject",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server",
    "build": "./node_modules/.bin/webpack",
    "lint": "./node_modules/.bin/eslint ./src"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint ./src",
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "basscss": "^8.0.10",
    "basscss-background-colors": "^2.1.0",
    "basscss-colors": "^2.2.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.1.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "postcss": "^7.0.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "eslint-config-prettier": "^3.1.0",
    "husky": "^1.1.2",
    "lint-staged": "^8.0.3"
  },
  "dependencies": {
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "whatwg-fetch": "^3.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
